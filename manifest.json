{
  "manifest_version": 3,
  "name": "Brancher",
  "version": "0.1.0",
  "description": "Generate branch names from Jira ticket URL",
  "action": {
    "default_icon": {
      "16": "images/cat.png",
      "24": "images/cat.png",
      "32": "images/cat.png"
    }
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["tabs", "storage", "activeTab", "scripting"]
}
